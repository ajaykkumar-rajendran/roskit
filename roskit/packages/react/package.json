{
  "name": "@roskit/react",
  "version": "0.1.0",
  "description": "React hooks and components for RosKit Bridge - Build ROS2 web apps with ease",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@roskit/client": "file:../client"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.0",
    "jsdom": "^24.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "keywords": [
    "ros",
    "ros2",
    "robotics",
    "react",
    "hooks",
    "websocket",
    "roskit"
  ],
  "author": "RosKit Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roskit/roskit.git",
    "directory": "packages/react"
  }
}
