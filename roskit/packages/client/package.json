{
  "name": "@roskit/client",
  "version": "0.1.0",
  "description": "TypeScript client SDK for RosKit Bridge - High-performance ROS2 web connectivity",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:conformance": "vitest run test/conformance.test.ts"
  },
  "dependencies": {
    "cbor-x": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {},
  "keywords": [
    "ros",
    "ros2",
    "robotics",
    "websocket",
    "typescript",
    "roskit"
  ],
  "author": "RosKit Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roskit/roskit.git",
    "directory": "packages/client"
  }
}
